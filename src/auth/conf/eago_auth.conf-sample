[main]
log_level = debug
secret_key = eago_default_secret_key
token_ttl = 900
admin_role_name = auth_admin

[etcd]
address = 127.0.0.1:2379
username = root
password = root

[iam]
iam_url = https://127.0.0.1/auth

[crowd]
crowd_url = http://127.0.0.1/crowd
app_name = eago
app_pass = eago

[mysql]
address = 127.0.0.1:3306
db_name = eago_auth
user = root
password = root
max_open_connections = 5
max_idle_connections = 20

[redis]
address = 127.0.0.1:6379
password = redis
db = 1

[log]
path = ./logs